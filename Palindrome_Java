//palindrome check
import java.util.*;
public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        StringBuilder str = new StringBuilder(A);
        str.reverse();
        String nw = str.toString();
        if (nw.equals(A)){
            System.out.println("Yes");
        }       
        else{
            System.out.print("No");
        }
        
    }
}

